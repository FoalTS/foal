    "foal",
    "session",
    "redis"
  ],
  "bugs": {
    "url": "https://github.com/FoalTS/foal/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FoalTS/foal"
  },
  "homepage": "https://github.com/FoalTS",
  "contributors": [
    "Lo√Øc Poullain <loic.poullain@centraliens.net>"
  ],
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "dependencies": {
    "@foal/core": "^1.2.0",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.43",
    "@types/node": "^10.5.6",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.2.0",
    "typescript": "^2.5.2"
  }
