{
  "name": "@foal/examples",
  "version": "0.6.0-alpha.1",
  "description": "FoalTs examples",
  "scripts": {
    "start": "node ./lib/server.js",
    "start:w": "supervisor -w ./lib/server.js --no-restart-on error ./lib/server.js",
    "test": "",
    "test:w": "mocha -w ./lib/test.js",
    "build": "foal build --mode=prod",
    "build:dev": "foal build --mode=dev",
    "build:dev:w": "foal build --mode=dev --watch",
    "build:test": "foal build --mode=test",
    "build:test:w": "foal build --mode=test --watch",
    "develop": "npm run build:dev && concurrently \"npm run build:dev:w\" \"npm run start:w\"",
    "develop:test": "npm run build:test && concurrently \"npm run build:test:w\" \"npm run test:w\"",
    "migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert"
  },
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "contributors": [
    "Lo√Øc Poullain <loic.poullain@centraliens.net>"
  ],
  "bugs": {
    "url": "https://github.com/FoalTS/foal/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FoalTS/foal"
  },
  "license": "MIT",
  "dependencies": {
    "@foal/core": "^0.6.0-alpha.1",
    "@foal/ejs": "^0.6.0-alpha.1",
    "bootstrap": "^4.0.0",
    "source-map-support": "^0.5.6",
    "sqlite3": "^4.0.0",
    "typeorm": "^0.2.6"
  },
  "devDependencies": {
    "@foal/cli": "^0.6.0-alpha.1",
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^10.1.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "html-loader": "^0.5.5",
    "mocha": "^4.0.1",
    "supervisor": "^0.12.0",
    "ts-loader": "^4.3.0",
    "ts-node": "^6.1.0",
    "typescript": "^2.5.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
