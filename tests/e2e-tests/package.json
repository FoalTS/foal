    "@foal/jwks-rsa": "^5.0.1",
    "@foal/jwt": "^5.0.1",
    "@foal/mongodb": "^5.0.1",
    "@foal/redis": "^5.0.1",
    "@foal/typeorm": "^5.0.1",
    "@foal/typestack": "^5.0.1",
    "@socket.io/redis-adapter": "~8.3.0",
    "ajv": "~8.17.1",
    "class-validator": "~0.14.0",
    "jsonwebtoken": "~9.0.2",
    "mongodb": "~5.9.2",
    "redis": "~4.7.0",
    "superagent": "~9.0.1",
    "supertest": "~7.1.1",
    "typeorm": "0.3.24",
    "yamljs": "~0.3.0"
  },
  "devDependencies": {
    "@types/mocha": "10.0.10",
